<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" styleClass="background2" stylesheets="@FarmGame.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.farming.view.GardenOverviewController">
   <children>
      <GridPane fx:id="gardenGrid" alignment="CENTER" gridLinesVisible="true" layoutX="135.0" layoutY="127.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="50.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="Grid00" mnemonicParsing="false" onAction="#handlePlantClick" text="Button" />
            <Button fx:id="Grid01" mnemonicParsing="false" onAction="#handlePlantClick" text="Button" GridPane.columnIndex="1" />
            <Button fx:id="Grid02" mnemonicParsing="false" onAction="#handlePlantClick" text="Button" GridPane.columnIndex="2" />
         </children>
      </GridPane>
      <Button layoutX="32.0" layoutY="426.0" mnemonicParsing="false" onAction="#returnHome" text="Return Home" />
      <Button layoutX="680.0" layoutY="354.0" mnemonicParsing="false" onAction="#getShop" text="Shop" />
      <Button layoutX="663.0" layoutY="413.0" mnemonicParsing="false" onAction="#getInventory" text="Inventory" />
      <Label fx:id="coinsLabel" layoutX="50.0" layoutY="50.0" text="Coin" />
      <Label fx:id="totalharvestplantLabel" layoutX="49.0" layoutY="93.0" text="Total Harvest Plant" />
      <Label layoutX="47.0" layoutY="250.0" text="Instruction" />
   </children>
</AnchorPane>
